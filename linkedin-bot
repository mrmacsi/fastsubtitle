var checker = setInterval(function () {$('div.chat-body').find('div').length;}, 5000)

var A = [['time','html']];
function addArray(){
	var selection = document.querySelector("#main > header > div.chat-body > div:nth-child(2) span") !== null;
	if(selection){
	A.push([new Date(), selection.innerHTML]);
	}
}
function getCSV() {
	var csvRows = [];
	if(A.length>1){
	for(var i=0, l=A.length; i<l; ++i){
		csvRows.push(A[i].join(','));
	}
	var csvString = csvRows.join("%0A");
	var a         = document.createElement('a');
	a.href        = 'data:attachment/csv,' + csvString;
	a.target      = '_blank';
	a.download    = 'myFile.csv';

	document.body.appendChild(a);
	a.click();
	document.body.removeChild(a);
	A = [['time','html']];
	}
}

setInterval(function(){ addArray(); }, 2000);
setInterval(function(){ getCSV(); }, 20000);

