var counter = $("li.mn-invitation-card.mn-person-card.mn-person-card--selectable.ember-view").length;
var now = 0;
var page = 1;
var personID = "";
function url(page){ $(".mn-invitation-pagination li:eq("+page+")").find("a").click();}
function go(count){
    if(now==100){page++;url(page);now=0;}
    if(personID!=""){
        $("#"+personID).find("button").click();
        console.log("withdraw "+personID);
        personID = "";
    }
    if(count>counter){
        var scroll = setInterval(function () {
            $('html, body').animate({scrollTop:$(document).height()},1000);
            counter = $("li.mn-invitation-card.mn-person-card.mn-person-card--selectable.ember-view").length;
            clearInterval(scroll);
            go(now);
        }, 1000);
    }else{
        var temp =$("li.mn-invitation-card.mn-person-card.mn-person-card--selectable.ember-view:eq("+count+")").attr("id");
        $("li.mn-invitation-card.mn-person-card.mn-person-card--selectable.ember-view:eq("+count+")").find("a").click();
        var gotimer = setInterval(function () {clearInterval(gotimer);
            console.log(count);
    	    var keys = ["france","india","tunisia","bangladesh","armenia","china"];
	    var text = $(".pv-top-card-section__location").text().toLowerCase().trim();
	    var exists = 0;
	    $.each(keys,function(key,value){if(text.indexOf(value)!=-1){exists++;}});
	    if(exists==0){
                personID = "";
	    }else{
                personID=temp;
		exists=0;
	    }
            var gotimer2 = setInterval(function () {
                clearInterval(gotimer2);
                if(window.history.state.path.indexOf("/in/")!=-1){history.back();}
                now++;
                var gotimer3 = setInterval(function () {
                    clearInterval(gotimer3);
                    go(now);
                }, 2000);
            }, 1000);
        }, 3000);
    }
}
go(now);

var counter=$("li.mn-invitation-card.mn-person-card.mn-person-card--selectable.ember-view").length,now=0,page=1,personID="";function url(n){$(".mn-invitation-pagination li:eq("+n+")").find("a").click()}function go(n){if(100==now&&(url(++page),now=0),""!=personID&&($("#"+personID).find("button").click(),console.log("withdraw "+personID),personID=""),n>counter)var e=setInterval(function(){$("html, body").animate({scrollTop:$(document).height()},1e3),counter=$("li.mn-invitation-card.mn-person-card.mn-person-card--selectable.ember-view").length,clearInterval(e),go(now)},1e3);else{var r=$("li.mn-invitation-card.mn-person-card.mn-person-card--selectable.ember-view:eq("+n+")").attr("id");$("li.mn-invitation-card.mn-person-card.mn-person-card--selectable.ember-view:eq("+n+")").find("a").click();var a=setInterval(function(){clearInterval(a),console.log(n);var e=$(".pv-top-card-section__location").text().toLowerCase().trim(),o=0;$.each(["france","india","tunisia","bangladesh","armenia"],function(n,r){-1!=e.indexOf(r)&&o++}),0==o?personID="":(personID=r,o=0);var t=setInterval(function(){clearInterval(t),-1!=window.history.state.path.indexOf("/in/")&&history.back(),now++;var n=setInterval(function(){clearInterval(n),go(now)},2e3)},1e3)},3e3)}}go(now);
