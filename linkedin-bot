var checker = setTimeout(check, 5000);
function check(){
if($('li.add').length==0){
 $('html, body').animate({scrollTop:$(document).height()},1000 ,function(){
	$(".search-result.search-result__occluded-item.ember-view").not(".remove").each(function(){
	var that = this,count=0, keys = ["software","recruitment","ceo","react","angular","java","cto "," ios","web","ux ","technology","android","programmer","coder","developer","symfony","laravel","front-end","stack","nodejs","php","asp","founder"],
	except = ['3d','business','é','chez','tunusia','mohammed','muhamed','ç','ş','ü','ğ','ö','ë','ï'], text = $(this).find(".name-and-icon").text().toLowerCase().trim() +' '+$(this).find(".subline-level-1.search-result__truncate").last().text().toLowerCase().trim(); 
	$.each(keys,function(key,value){if(text.indexOf(value)!=-1){count++;}});
	$.each(except,function(key,value){if(text.indexOf(value)!=-1){count=0;}});
	if(count==0){$(that).addClass("remove").find(".lazy-image").css({'border': '3px solid red'});
	}else{
	$(that).addClass("add").find(".lazy-image").css({'border': '3px solid green'});count=0;}
	});
	add();
});
}else{
    	clearTimeout(checker);
}
}

var add = function loop() {
    var rand = Math.round(Math.random() * 4400) + 6100;
    var try1 = setTimeout(function() {
    	console.log(rand)
	if($('li.add').length>0){
	$('li.add').first().find(".button-secondary-medium.m5").click();
	$('.button-secondary-large.mr1').click();
	$('.send-invite__custom-message.mb3.ember-text-area.ember-view').val("Dear "+$('li.add').first().find(".name-and-icon").text().trim()+", I am Software Engineer in London. I have experience in a lot of web programming language. Would like to see you in my connections. Thanks & Bests. Macit Simsek");
	$(".button-primary-large.ml1").click();
	$('li.add').first().removeClass("add");
	add(); 
	}else{
	$('.page-list').next().find('button').click();
	check();
    	} 
    	clearTimeout(try1);
    }, rand);
}; 

var checker = setInterval(function () {
$(".search-result.search-result__occluded-item.ember-view").not(".remove").each(function(){
	if($(this).find(".button-secondary-medium.m5").prop("disabled")){$(this).removeClass("add")}
	var that = this,count=0, keys = ["software","recruitment","ceo","react","angular","java","cto "," ios","web","ux ","technology","android","programmer","coder","developer","symfony","laravel","front-end","stack","nodejs","php","asp","founder"],
	except = ['3d','business','é','chez','tunusia','mohammed','muhamed','ç','ş','ü','ğ','ö','ë','ï'], text = $(this).find(".name-and-icon").text().toLowerCase().trim() +' '+$(this).find(".subline-level-1.search-result__truncate").last().text().toLowerCase().trim(); 
	$.each(keys,function(key,value){if(text.indexOf(value)!=-1){count++;}});
	$.each(except,function(key,value){if(text.indexOf(value)!=-1){count=0;}});
	if(count==0){$(that).addClass("remove").find(".lazy-image").css({'border': '3px solid red'});
	}else{
	$(that).addClass("add").find(".lazy-image").css({'border': '3px solid green'});count=0;}
	});
}, 1000);

var add = setInterval(function () {
if($('li.add').length>0){
	$('li.add').first().find(".button-secondary-medium.m5").click();
	$('.button-secondary-large.mr1').click();
	$('.send-invite__custom-message.mb3.ember-text-area.ember-view').val("Dear "+$('li.add').first().find(".name-and-icon").text().trim()+", I am Software Engineer in London. I have experience in a lot of web programming language. Would like to see you in my connections. Thanks & Bests. Macit Simsek");
	$(".button-primary-large.ml1").prop("disabled",false).click();
	$('li.add').first().removeClass("add").addClass("remove").find(".lazy-image").css({'border': '3px solid red'});
}else{
	$('.page-list').next().find('button').click();
} 
}, 2000);
