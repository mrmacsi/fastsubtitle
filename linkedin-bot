var counter = $("li.mn-person-card.connection-card.ember-view").length;
var now = 0;
function go(count){
    if(count>counter){
        var scroll = setInterval(function () {
            $('html, body').animate({scrollTop:$(document).height()},1000);
            counter = $("li.mn-person-card.connection-card.ember-view").length;
            clearInterval(scroll);
            go(now);
        }, 1000);
    }else{
        $("li.mn-person-card.connection-card.ember-view:eq("+count+")").find("a").click();
        var gotimer = setInterval(function () {clearInterval(gotimer);
            console.log(count);
	    var keys = ["france","india","tunisia","bangladesh","armenia","china"];
	    var text = $(".pv-top-card-section__location").text().toLowerCase().trim();
	    var exists = 0;
	    $.each(keys,function(key,value){if(text.indexOf(value)!=-1){exists++;}});
	    if(exists==0){
                if(window.history.state.path.indexOf("/in/")!=-1){history.back();}
                now++;
                var gotimer3 = setInterval(function () {
                    clearInterval(gotimer3);
                    go(now);
                }, 1000);
	    }else{
                $(".pv-s-profile-actions__overflow-toggle").click();
                $(".pv-s-profile-actions__label:eq(3)").click();
                $(".pv-s-profile-actions__label:eq(5)").click();
                var gotimer2 = setInterval(function () {
                    clearInterval(gotimer2);
                    if(window.history.state.path.indexOf("/in/")!=-1){history.back();}
                    now++;
                    var gotimer3 = setInterval(function () {
                        clearInterval(gotimer3);
                        go(now);
                    }, 1000);
                }, 1000);
		exists=0;
	    }
        }, 2500);
    }
}
go(now);

var counter=$("li.mn-person-card.connection-card.ember-view").length,now=0;function go(e){if(e>counter)var n=setInterval(function(){$("html, body").animate({scrollTop:$(document).height()},1e3),counter=$("li.mn-person-card.connection-card.ember-view").length,clearInterval(n),go(now)},1e3);else{$("li.mn-person-card.connection-card.ember-view:eq("+e+")").find("a").click();var o=setInterval(function(){clearInterval(o),console.log(e);var n=$(".pv-top-card-section__location").text().toLowerCase().trim(),t=0;if($.each(["france","india","tunisia","bangladesh","armenia"],function(e,o){-1!=n.indexOf(o)&&t++}),0==t){-1!=window.history.state.path.indexOf("/in/")&&history.back(),now++;var a=setInterval(function(){clearInterval(a),go(now)},1e3)}else{$(".pv-s-profile-actions__overflow-toggle").click(),$(".pv-s-profile-actions__label:eq(3)").click(),$(".pv-s-profile-actions__label:eq(5)").click();var r=setInterval(function(){clearInterval(r),-1!=window.history.state.path.indexOf("/in/")&&history.back(),now++;var e=setInterval(function(){clearInterval(e),go(now)},1e3)},1e3);t=0}},2500)}}go(now);

